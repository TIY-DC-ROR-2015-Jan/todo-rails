<%= form_for @list do |f| %>
  <%= render partial: 'partials/errors', locals: { object: @list } %>

  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
